{
  "extends": "airbnb",
  "plugins": [ "jest" ],
  "env": {
    "jest/globals": true,
    "browser": true,
	"node": true,
  },
  "rules" : {
  	"react/prop-types": [0],
  	"no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
  	"linebreak-style" :["error", "windows"],
  	"import/no-extraneous-dependencies": ["error", {"devDependencies": true }],
  }
}
